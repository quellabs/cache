{
  "name": "quellabs/support",
  "description": "Support functions for the Quellabs ecosystem",
  "type": "library",
  "license": "MIT",
  "keywords": ["composer", "utilities", "psr-4"],
  "config": {
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/*": true
    }
  },
  "require": {
    "php": "^8.2",
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Quellabs\\Support\\": "src/"
    }
  }
}